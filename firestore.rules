rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Открытый доступ для приложения
    match /config/{document=**} {
      allow read, write;
    }
    match /balance/{document=**} {
      allow read, write;
    }
    match /locations/{document=**} {
      allow read, write;
    }
  }
}